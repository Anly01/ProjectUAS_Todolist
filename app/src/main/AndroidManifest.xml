        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="To Do List"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BUTTONS">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".todoPage" />
        <activity android:name=".calendarPage" />
        <activity android:name=".shopPage"/>
        <activity android:name=".helpPage"/>
        <activity android:name=".AboutUs"/>
        <activity android:name=".Faq"/>
        <activity android:name=".themePage"/>
        <activity android:name=".buttonPage"/>

    </application>



</manifest>